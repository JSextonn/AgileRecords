<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox alignment="TOP_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="523.0" prefWidth="855.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="main.java.controller.AdminViewController">
    <children>
        <TableView fx:id="adminTableView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="idTableColumn" prefWidth="61.0" text="ID"/>
                <TableColumn fx:id="usernameTableColumn" prefWidth="115.0" text="Username"/>
                <TableColumn fx:id="firstNameTableColumn" prefWidth="100.0" text="First Name"/>
                <TableColumn fx:id="lastNameTableColumn" prefWidth="100.0" text="Last Name"/>
                <TableColumn fx:id="majorTableColumn" prefWidth="150.0" text="Major"/>
                <TableColumn fx:id="GPATableColumn" prefWidth="43.0" text="GPA"/>
            </columns>
        </TableView>
        <ButtonBar prefHeight="40.0" prefWidth="200.0">
            <buttons>
                <Button fx:id="addButton" mnemonicParsing="false" onAction="#handleAddButtonAction" text="Add"
                        ButtonBar.buttonData="LEFT"/>
                <Button fx:id="removeButton" mnemonicParsing="false" onAction="#handleRemoveButtonAction" text="Remove"
                        ButtonBar.buttonData="LEFT"/>
                <Button fx:id="refreshButton" mnemonicParsing="false" onAction="#handleRefreshButton" text="Refresh"/>
            </buttons>
        </ButtonBar>
        <AnchorPane>
            <children>
                <Label text="Status: "/>
                <Label fx:id="statusLabel" layoutX="39.0" text="Status"/>
                <Label fx:id="usernameLabel" layoutX="659.0" text="Username" AnchorPane.rightAnchor="0.0"/>
                <Label layoutX="599.0" text="Username: " AnchorPane.rightAnchor="54.0"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
