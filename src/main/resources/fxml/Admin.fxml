<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.AdminController">
    <children>
        <TableView fx:id="adminTableView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="idTableColumn" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="firstNameTableColumn" prefWidth="75.0" text="First Name" />
                <TableColumn fx:id="lastNameTableColumn" prefWidth="75.0" text="Last Name" />
            </columns>
        </TableView>
        <ButtonBar prefHeight="40.0" prefWidth="200.0">
            <buttons>
                <Button fx:id="addButton" mnemonicParsing="false" onAction="#openCreateStudent" text="Add" ButtonBar.buttonData="LEFT" />
                <Button fx:id="removeButton" mnemonicParsing="false" onAction="#removeSelected" text="Remove" ButtonBar.buttonData="LEFT" />
            </buttons>
        </ButtonBar>
        <Pane>
            <children>
                <Label fx:id="statusLabel" layoutY="2.0" text="Status: " />
                <Label layoutX="416.0" layoutY="2.0" text="Logged In As: " />
            <Label fx:id="loggedInAsLabel" layoutX="492.0" layoutY="1.0" prefHeight="17.0" prefWidth="0.0" />
            </children>
        </Pane>
    </children>
</VBox>
